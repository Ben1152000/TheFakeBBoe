<p id="notice"><%= notice %></p>

<h1>User Feed Subscriptions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_feed_subscriptions.each do |user_feed_subscription| %>
      <tr>
        <td><%= link_to 'Show', user_feed_subscription %></td>
        <td><%= link_to 'Edit', edit_user_feed_subscription_path(user_feed_subscription) %></td>
        <td><%= link_to 'Destroy', user_feed_subscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Feed Subscription', new_user_feed_subscription_path %>
